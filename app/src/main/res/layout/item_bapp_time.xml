<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ibt_rl"
    android:layout_width="100dp"
    android:layout_height="60dp"
    android:layout_margin="@dimen/dimen_mva_margin_Top"
    android:background="@drawable/shape_round_ibt">

    <TextView
        android:id="@+id/ibt_tv_week"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/ipt_tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="@dimen/dimen_ibt_top" />


</RelativeLayout>
